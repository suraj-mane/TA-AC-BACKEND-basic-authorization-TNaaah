<%- include('partials/header.ejs')%>
<div class="container">
  <form class="from" action="/users/<%= article.id%>/edit" method="post">
    <h1>Add articles</h1>
    <input class="input" type="text" name="title" placeholder="Enter your title" value="<%= article.title%>">
    <input class="input" type="text" name="tags" placeholder="Enter your tags" value="<%= article.tags%>">
    <input class="input" type="text" name="summery" placeholder="Enter your summery" value="<%= article.summery%>">
    <button type="submit" class="btn btn-primary">Edit</button>
  </form>
</div>
<%- include('partials/footer.ejs')%>